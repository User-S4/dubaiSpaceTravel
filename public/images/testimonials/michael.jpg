<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="300" height="300" viewBox="0 0 300 300" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#0A1128" />
  
  <!-- Profile Circle -->
  <circle cx="150" cy="140" r="80" fill="#1C2541" stroke="#E6C026" stroke-width="2" />
  
  <!-- Silhouette -->
  <g fill="#3A506B">
    <!-- Head -->
    <circle cx="150" cy="110" r="40" />
    <!-- Short Hair -->
    <path d="M115 85 Q150 70 185 85" stroke="#3A506B" stroke-width="12" fill="none" />
    <!-- Body -->
    <path d="M120 150 Q150 200 180 150 L180 200 L120 200 Z" />
  </g>
  
  <!-- Astronaut Elements -->
  <g transform="translate(200, 110)">
    <!-- Helmet Outline -->
    <circle cx="0" cy="0" r="20" fill="none" stroke="#E6C026" stroke-width="2" />
    <!-- Visor -->
    <path d="M-12 -5 Q0 10 12 -5" fill="none" stroke="#E6C026" stroke-width="2" />
    <!-- Helmet Detail -->
    <circle cx="0" cy="-10" r="3" fill="#E6C026" />
  </g>
  
  <!-- Space Medal -->
  <g transform="translate(120, 100)">
    <circle cx="0" cy="0" r="10" fill="none" stroke="#E6C026" stroke-width="1.5" />
    <circle cx="0" cy="0" r="5" fill="#E6C026" />
    <line x1="0" y1="-14" x2="0" y2="-10" stroke="#E6C026" stroke-width="1.5" />
    <line x1="0" y1="10" x2="0" y2="14" stroke="#E6C026" stroke-width="1.5" />
    <line x1="-14" y1="0" x2="-10" y2="0" stroke="#E6C026" stroke-width="1.5" />
    <line x1="10" y1="0" x2="14" y2="0" stroke="#E6C026" stroke-width="1.5" />
  </g>
  
  <!-- Decorative Elements - Stars -->
  <g fill="#E6C026">
    <circle cx="50" cy="40" r="3" />
    <circle cx="250" cy="50" r="2" />
    <circle cx="70" cy="245" r="4" />
    <circle cx="230" cy="255" r="3" />
  </g>
  
  <!-- Name Label -->
  <text x="150" y="240" font-family="Arial, sans-serif" font-size="16" fill="#FFFFFF" text-anchor="middle">Michael Rodriguez</text>
  <text x="150" y="260" font-family="Arial, sans-serif" font-size="12" fill="#E6C026" text-anchor="middle">Retired Astronaut</text>
</svg> 