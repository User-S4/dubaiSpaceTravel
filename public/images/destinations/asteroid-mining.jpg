<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Space Background -->
  <rect width="600" height="400" fill="#0A1128" />
  
  <!-- Stars -->
  <g fill="#FFFFFF">
    <!-- Random Small Stars -->
    <circle cx="40" cy="35" r="1" />
    <circle cx="110" cy="75" r="1.2" />
    <circle cx="190" cy="45" r="0.8" />
    <circle cx="280" cy="130" r="1" />
    <circle cx="340" cy="50" r="1.5" />
    <circle cx="410" cy="35" r="0.7" />
    <circle cx="470" cy="95" r="1.3" />
    <circle cx="540" cy="30" r="1" />
    <circle cx="70" cy="170" r="1.2" />
    <circle cx="160" cy="230" r="0.9" />
    <circle cx="220" cy="310" r="1.1" />
    <circle cx="330" cy="340" r="0.8" />
    <circle cx="390" cy="290" r="1.4" />
    <circle cx="440" cy="330" r="1" />
    <circle cx="530" cy="360" r="1.2" />
    <circle cx="40" cy="320" r="0.7" />
    
    <!-- Brighter Stars -->
    <circle cx="90" cy="60" r="2" opacity="0.9" />
    <circle cx="260" cy="90" r="2.5" opacity="0.9" />
    <circle cx="410" cy="80" r="2.2" opacity="0.9" />
    <circle cx="170" cy="240" r="2.3" opacity="0.9" />
    <circle cx="380" cy="240" r="2" opacity="0.9" />
    <circle cx="490" cy="270" r="2.4" opacity="0.9" />
  </g>
  
  <!-- Asteroid -->
  <g transform="translate(280, 200)">
    <!-- Main Asteroid Body with Irregular Shape -->
    <path d="M0,0 L40,-20 L80,10 L70,50 L30,70 L-20,60 L-50,30 L-40,-10 Z" fill="#6b6764" />
    
    <!-- Asteroid Surface Details -->
    <ellipse cx="-10" cy="30" rx="15" ry="10" fill="#524f4d" />
    <ellipse cx="40" cy="20" rx="12" ry="8" fill="#524f4d" />
    <ellipse cx="0" cy="-10" rx="10" ry="7" fill="#524f4d" />
    
    <!-- Craters -->
    <circle cx="20" cy="-5" r="8" fill="#3e3c3a" />
    <circle cx="-30" cy="10" r="6" fill="#3e3c3a" />
    <circle cx="50" cy="30" r="5" fill="#3e3c3a" />
    
    <!-- Mineral Veins (Golden) -->
    <path d="M-20,0 L20,10 L40,0" fill="none" stroke="#E6C026" stroke-width="2" />
    <path d="M10,30 L30,-10" fill="none" stroke="#E6C026" stroke-width="2" />
    <path d="M-30,20 L-10,-5" fill="none" stroke="#E6C026" stroke-width="2" />
    
    <!-- Sparkling Points (Valuable Minerals) -->
    <circle cx="-15" cy="5" r="2" fill="#E6C026" />
    <circle cx="25" cy="5" r="2" fill="#E6C026" />
    <circle cx="35" cy="-5" r="2" fill="#E6C026" />
    <circle cx="15" cy="25" r="2" fill="#E6C026" />
    <circle cx="-25" cy="15" r="2" fill="#E6C026" />
  </g>
  
  <!-- Mining Spacecraft -->
  <g transform="translate(400, 180)">
    <!-- Main Ship Body -->
    <rect x="-40" y="-20" width="80" height="40" fill="#3A506B" rx="5" stroke="#E6C026" stroke-width="1" />
    
    <!-- Mining Equipment Arm -->
    <path d="M-40,0 L-80,20 L-100,10" fill="none" stroke="#5BC0BE" stroke-width="3" />
    <circle cx="-100" cy="10" r="10" fill="#3A506B" stroke="#E6C026" stroke-width="1" />
    <rect x="-105" y="10" width="10" height="15" fill="#5BC0BE" />
    
    <!-- Mining Laser -->
    <line x1="-100" y1="25" x2="-110" y2="40" stroke="#E6C026" stroke-width="2" />
    <circle cx="-110" cy="40" r="3" fill="#E6C026" />
    
    <!-- Command Module -->
    <rect x="10" y="-30" width="20" height="10" fill="#1C2541" stroke="#E6C026" stroke-width="1" rx="2" />
    <circle cx="20" cy="-25" r="3" fill="#5BC0BE" />
    
    <!-- Thrusters -->
    <rect x="-30" y="20" width="10" height="5" fill="#E6C026" />
    <rect x="-10" y="20" width="10" height="5" fill="#E6C026" />
    <rect x="10" y="20" width="10" height="5" fill="#E6C026" />
    <rect x="30" y="20" width="10" height="5" fill="#E6C026" />
    
    <!-- Antenna -->
    <line x1="40" y1="-20" x2="55" y2="-35" stroke="#E6C026" stroke-width="1" />
    <circle cx="55" cy="-35" r="5" fill="none" stroke="#E6C026" stroke-width="1" />
    
    <!-- Solar Array -->
    <rect x="40" y="-15" width="40" height="30" fill="#5BC0BE" stroke="#E6C026" stroke-width="1" />
    <line x1="45" y1="-15" x2="45" y2="15" stroke="#E6C026" stroke-width="1" />
    <line x1="55" y1="-15" x2="55" y2="15" stroke="#E6C026" stroke-width="1" />
    <line x1="65" y1="-15" x2="65" y2="15" stroke="#E6C026" stroke-width="1" />
    <line x1="75" y1="-15" x2="75" y2="15" stroke="#E6C026" stroke-width="1" />
  </g>
  
  <!-- Small Transport Ship -->
  <g transform="translate(180, 130)">
    <!-- Ship Body -->
    <rect x="-15" y="-10" width="30" height="20" fill="#3A506B" rx="5" stroke="#E6C026" stroke-width="1" />
    
    <!-- Windows -->
    <circle cx="-5" cy="0" r="3" fill="#5BC0BE" />
    <circle cx="5" cy="0" r="3" fill="#5BC0BE" />
    
    <!-- Thrusters -->
    <rect x="-10" y="10" width="5" height="3" fill="#E6C026" />
    <rect x="5" y="10" width="5" height="3" fill="#E6C026" />
    
    <!-- Flight Path to Asteroid -->
    <path d="M0,10 Q 50,30 100,70" fill="none" stroke="#5BC0BE" stroke-width="1" stroke-dasharray="3,3" />
  </g>
  
  <!-- Earth (Small in Distance) -->
  <g transform="translate(80, 80)">
    <circle cx="0" cy="0" r="30" fill="#1a3761" />
    <path d="M-25,-10 Q -10,10 -20,25" fill="#3a7d5c" />
    <path d="M-5,-25 Q 15,-15 10,10" fill="#3a7d5c" />
    <circle cx="0" cy="0" r="35" fill="none" stroke="#88b7dd" stroke-width="3" opacity="0.3" />
  </g>
  
  <!-- Title -->
  <text x="300" y="50" font-family="Arial, sans-serif" font-size="30" fill="#FFFFFF" text-anchor="middle" stroke="#E6C026" stroke-width="1">Asteroid Mining Operation</text>
</svg> 