<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background - Mars Surface -->
  <rect width="600" height="400" fill="#a94426" />
  
  <!-- Mars Sky -->
  <rect width="600" height="200" fill="#d68d77" />
  
  <!-- Mars Surface Texture -->
  <g fill="#863a20">
    <ellipse cx="100" cy="350" rx="80" ry="30" />
    <ellipse cx="300" cy="380" rx="120" ry="40" />
    <ellipse cx="500" cy="360" rx="90" ry="25" />
    <ellipse cx="200" cy="390" rx="100" ry="30" />
    <ellipse cx="400" cy="370" rx="70" ry="20" />
  </g>
  
  <!-- Mars Base Structure -->
  <g transform="translate(300, 250)">
    <!-- Central Dome -->
    <ellipse cx="0" cy="0" rx="70" ry="40" fill="#1C2541" stroke="#E6C026" stroke-width="2" />
    <ellipse cx="0" cy="0" rx="50" ry="30" fill="#0A1128" stroke="#E6C026" stroke-width="1" />
    
    <!-- Connected Domes -->
    <g>
      <!-- Left Dome -->
      <ellipse cx="-100" cy="20" rx="40" ry="25" fill="#1C2541" stroke="#E6C026" stroke-width="2" />
      <!-- Right Dome -->
      <ellipse cx="100" cy="20" rx="40" ry="25" fill="#1C2541" stroke="#E6C026" stroke-width="2" />
      <!-- Connector Tubes -->
      <rect x="-60" y="10" width="20" height="20" fill="#3A506B" stroke="#E6C026" stroke-width="1" />
      <rect x="40" y="10" width="20" height="20" fill="#3A506B" stroke="#E6C026" stroke-width="1" />
    </g>
    
    <!-- Mars Rover -->
    <g transform="translate(-150, 50)">
      <rect x="-20" y="-10" width="40" height="20" fill="#3A506B" stroke="#E6C026" stroke-width="1" rx="5" />
      <circle cx="-15" cy="15" r="8" fill="#1C2541" stroke="#E6C026" stroke-width="1" />
      <circle cx="15" cy="15" r="8" fill="#1C2541" stroke="#E6C026" stroke-width="1" />
      <rect x="-10" y="-20" width="20" height="10" fill="#3A506B" stroke="#E6C026" stroke-width="1" />
    </g>
    
    <!-- Solar Array -->
    <g transform="translate(150, 40)">
      <rect x="-40" y="0" width="80" height="30" fill="#5BC0BE" stroke="#E6C026" stroke-width="1" />
      <line x1="-30" y1="0" x2="-30" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="-20" y1="0" x2="-20" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="-10" y1="0" x2="-10" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="0" y1="0" x2="0" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="10" y1="0" x2="10" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="20" y1="0" x2="20" y2="30" stroke="#E6C026" stroke-width="1" />
      <line x1="30" y1="0" x2="30" y2="30" stroke="#E6C026" stroke-width="1" />
    </g>
  </g>
  
  <!-- Distant Mountains -->
  <path d="M0 200 L50 170 L100 190 L150 160 L200 180 L250 150 L300 180 L350 140 L400 170 L450 150 L500 180 L550 160 L600 190 L600 200 Z" fill="#713520" />
  
  <!-- Mars Sky Details -->
  <g>
    <!-- Phobos (Moon) -->
    <circle cx="100" cy="80" r="15" fill="#bbaa99" />
    <circle cx="105" cy="75" r="3" fill="#a59989" />
    <circle cx="95" cy="85" r="4" fill="#a59989" />
    
    <!-- Deimos (Smaller Moon) -->
    <circle cx="500" cy="60" r="7" fill="#bbaa99" />
    
    <!-- Dust Clouds -->
    <ellipse cx="200" cy="150" rx="80" ry="20" fill="#c27c65" opacity="0.5" />
    <ellipse cx="400" cy="130" rx="100" ry="25" fill="#c27c65" opacity="0.3" />
  </g>
  
  <!-- Astronaut Figure -->
  <g transform="translate(450, 280)">
    <!-- Spacesuit -->
    <rect x="-10" y="-5" width="20" height="30" fill="#3A506B" rx="5" />
    <!-- Helmet -->
    <circle cx="0" cy="-15" r="10" fill="#3A506B" stroke="#E6C026" stroke-width="1" />
    <!-- Visor -->
    <ellipse cx="0" cy="-15" rx="7" ry="5" fill="#5BC0BE" />
    <!-- Backpack -->
    <rect x="-12" y="-5" width="24" height="15" fill="#1C2541" rx="2" />
    <!-- Legs -->
    <rect x="-8" y="25" width="6" height="15" fill="#3A506B" />
    <rect x="2" y="25" width="6" height="15" fill="#3A506B" />
    <!-- Arms -->
    <rect x="-15" y="0" width="5" height="15" fill="#3A506B" />
    <rect x="10" y="0" width="5" height="15" fill="#3A506B" />
  </g>
  
  <!-- Title -->
  <text x="300" y="50" font-family="Arial, sans-serif" font-size="30" fill="#FFFFFF" text-anchor="middle" stroke="#E6C026" stroke-width="1">Mars Base One</text>
</svg> 