<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Space Background -->
  <rect width="600" height="400" fill="#0A1128" />
  
  <!-- Stars -->
  <g fill="#FFFFFF">
    <!-- Random Small Stars -->
    <circle cx="50" cy="30" r="1" />
    <circle cx="120" cy="85" r="1.2" />
    <circle cx="200" cy="40" r="0.8" />
    <circle cx="270" cy="120" r="1" />
    <circle cx="350" cy="60" r="1.5" />
    <circle cx="400" cy="25" r="0.7" />
    <circle cx="480" cy="90" r="1.3" />
    <circle cx="550" cy="40" r="1" />
    <circle cx="80" cy="180" r="1.2" />
    <circle cx="150" cy="220" r="0.9" />
    <circle cx="230" cy="300" r="1.1" />
    <circle cx="320" cy="350" r="0.8" />
    <circle cx="380" cy="280" r="1.4" />
    <circle cx="450" cy="320" r="1" />
    <circle cx="520" cy="370" r="1.2" />
    <circle cx="30" cy="330" r="0.7" />
    
    <!-- Brighter Stars -->
    <circle cx="100" cy="50" r="2" opacity="0.9" />
    <circle cx="250" cy="100" r="2.5" opacity="0.9" />
    <circle cx="420" cy="70" r="2.2" opacity="0.9" />
    <circle cx="180" cy="250" r="2.3" opacity="0.9" />
    <circle cx="370" cy="230" r="2" opacity="0.9" />
    <circle cx="500" cy="280" r="2.4" opacity="0.9" />
  </g>
  
  <!-- Venus -->
  <g transform="translate(300, 200)">
    <!-- Planet Base -->
    <circle cx="0" cy="0" r="120" fill="#e6b87f" />
    
    <!-- Cloud Patterns -->
    <g opacity="0.7">
      <ellipse cx="-40" cy="-30" rx="70" ry="40" fill="#f0d6a3" />
      <ellipse cx="50" cy="20" rx="90" ry="50" fill="#f0d6a3" />
      <ellipse cx="-30" cy="60" rx="60" ry="35" fill="#f0d6a3" />
      <ellipse cx="30" cy="-70" rx="50" ry="30" fill="#f0d6a3" />
    </g>
    
    <!-- Venus Surface Hints (through clouds) -->
    <g opacity="0.3">
      <ellipse cx="-60" cy="40" rx="20" ry="15" fill="#c9894a" />
      <ellipse cx="70" cy="-50" rx="25" ry="20" fill="#c9894a" />
      <ellipse cx="30" cy="80" rx="15" ry="10" fill="#c9894a" />
    </g>
    
    <!-- Atmospheric Glow -->
    <circle cx="0" cy="0" r="130" fill="none" stroke="#f8e9c6" stroke-width="10" opacity="0.5" />
  </g>
  
  <!-- Spacecraft -->
  <g transform="translate(480, 150)">
    <!-- Main Body -->
    <rect x="-30" y="-15" width="60" height="30" fill="#3A506B" rx="5" stroke="#E6C026" stroke-width="1" />
    
    <!-- Solar Panels -->
    <rect x="-80" y="-5" width="50" height="20" fill="#5BC0BE" stroke="#E6C026" stroke-width="1" />
    <rect x="30" y="-5" width="50" height="20" fill="#5BC0BE" stroke="#E6C026" stroke-width="1" />
    
    <!-- Antenna -->
    <line x1="0" y1="-15" x2="0" y2="-35" stroke="#E6C026" stroke-width="1" />
    <circle cx="0" cy="-35" r="5" fill="none" stroke="#E6C026" stroke-width="1" />
    
    <!-- Thrusters -->
    <rect x="-20" y="15" width="10" height="10" fill="#E6C026" />
    <rect x="10" y="15" width="10" height="10" fill="#E6C026" />
    
    <!-- Windows -->
    <circle cx="-15" cy="0" r="5" fill="#5BC0BE" />
    <circle cx="0" cy="0" r="5" fill="#5BC0BE" />
    <circle cx="15" cy="0" r="5" fill="#5BC0BE" />
  </g>
  
  <!-- Flight Path -->
  <path d="M480 150 Q 400 130 300 200 Q 200 270 120 250" fill="none" stroke="#E6C026" stroke-width="1" stroke-dasharray="5,5" />
  
  <!-- Sun (partial view) -->
  <circle cx="0" cy="200" r="100" fill="#f9d71c" />
  <circle cx="0" cy="200" r="120" fill="none" stroke="#f9d71c" stroke-width="10" opacity="0.3" />
  
  <!-- Title -->
  <text x="300" y="50" font-family="Arial, sans-serif" font-size="30" fill="#FFFFFF" text-anchor="middle" stroke="#E6C026" stroke-width="1">Venus Flyby Experience</text>
</svg> 